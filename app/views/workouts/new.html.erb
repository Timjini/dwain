

<%= form_for @workout do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.text_field :video_link%>
  <%= f.hidden_field :coach_id, value: current_coach.id %>
  <span> video </span>
  <%=f.file_field :video %>
  <span> Image </span>
  <%=f.file_field :image%>
  <%= f.submit %>
<%end%>